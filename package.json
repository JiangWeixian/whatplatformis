{
  "name": "whatplatformis",
  "version": "0.1.0",
  "license": "MIT",
  "packageManager": "pnpm@7.23.0",
  "homepage": "https://github.com/JiangWeixian/whatplatformis#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/whatplatformis.git"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/whatplatformis/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": {
    "name": "JW",
    "email": "jiangweixian1994@gmail.com",
    "url": "https://twitter.com/jiangweixian"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "require": "./dist/browser.cjs",
        "import": "./dist/browser.mjs",
        "types": "./dist/exports/browser.d.ts"
      },
      "default": {
        "require": "./dist/default.cjs",
        "import": "./dist/default.mjs",
        "types": "./dist/exports/default.d.ts"
      },
      "node": {
        "require": "./dist/node.cjs",
        "import": "./dist/node.mjs",
        "types": "./dist/exports/node.d.ts"
      }
    }
  },
  "main": "dist/node.cjs",
  "module": "dist/node.mjs",
  "types": "dist/exports/node.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@aiou/eslint-config": "0.7.8",
    "@changesets/cli": "2.21.1",
    "@rollup/plugin-alias": "4.0.3",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/node": "17.0.22",
    "cz-emoji": "1.3.1",
    "esbuild": "^0.17.12",
    "eslint": "8.11.0",
    "esrua": "0.2.1",
    "husky": "7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "11.2.6",
    "npm-run-all": "^4.1.5",
    "publish-police": "^0.0.3",
    "rimraf": "3.0.2",
    "rollup": "3.19.1",
    "rollup-plugin-condition-exports": "2.0.0-next.3",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "9.1.2",
    "rollup-plugin-node-externals": "5.1.2",
    "ts-node": "^10.7.0",
    "tslib": "2.4.0",
    "ttypescript": "1.5.13",
    "typescript": "4.4.3",
    "typescript-transform-paths": "3.3.1",
    "vitest": "0.10.0"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build:lib": "rollup -c --silent",
    "build:dts": "ttsc",
    "build": "rimraf dist && run-p build:*",
    "dev": "rollup -c -w --silent",
    "test": "vitest",
    "pretest": "pnpm run build",
    "prepublishOnly": "publish-police",
    "release": "pnpm run build && np --no-cleanup --yolo --no-publish --any-branch",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  }
}
